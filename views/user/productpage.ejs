<script>
    var quantity = 1;
  var selectedSize = '7';  // Default selected size

  function updateQuantityDisplay() {
    const quantityElement = document.getElementById('quantity');
    quantityElement.innerText = quantity;
  }

  function incrementQuantity(totalstock) {
    if (quantity >= totalstock) {
        Swal.fire({
  icon: 'info',
  title: 'Out of stock',
  text:"Product is not available",
  showConfirmButton: false,
  timer: 1500,  // Automatically close after 1.5 seconds
  backdrop: `
    rgba(0,0,123,0.4)
    url('path/to/your-image.jpg')
    left top
    no-repeat
  `,
  customClass: {
    title: 'my-title-class',
    icon: 'my-icon-class'
  }
  
});
return;
          
          
        }
    quantity++;
    updateQuantityDisplay();
  }

  function decrementQuantity() {
    if (quantity > 1) {
      quantity--;
      updateQuantityDisplay();
    }
  }

  document.addEventListener('DOMContentLoaded', () => {
    const sizeOptions = document.querySelectorAll('.list-filter.size-filter a');
    const productId = document.getElementById('addToCartButton').dataset.productId;

    sizeOptions.forEach(sizeOption => {
      sizeOption.addEventListener('click', () => {
        selectedSize = sizeOption.innerText;
      });
    });

    document.getElementById('addToCartButton').addEventListener('click', () => {
        const productDetails = {
            productId: productId,
            size: selectedSize,
            quantity: quantity
        };
        console.log('Product details to be added to cart:', productDetails);

        // Send product details to the server
        sendProductDetailsToServer(productDetails);
    });
});

function sendProductDetailsToServer(productDetails) {
    fetch(`/addtocart/${productDetails.productId}`, {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(productDetails)
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Network response was not ok');
        }
        return response.json();
    })
    .then(data => {
        console.log('Product details sent to the server:', data);

        // Check if a redirect URL is provided in the response
        if (data.redirectUrl) {
            window.location.href = data.redirectUrl;  // Redirect to the provided URL
        }
    })
    .catch(error => {
        console.error('Error:', error);
    });
}

</script>
